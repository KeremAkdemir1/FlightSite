<body style="background-image:url(https://p4.wallpaperbetter.com/wallpaper/993/439/626/the-sky-clouds-flight-nature-wallpaper-preview.jpg);background-repeat:no-repeat;background-size:cover">
    <div class="total-content">
        <div class="list_of_members">
            <div class="sales" style="background-color:#1abc9c">
                <div class="icon-text">
                    <h3>@ViewBag.d1</h3>
                    <p>Count Of All Flights</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="new-users" style="background-color:#2ecc71">

                <div class="icon-text">
                    <h3>@ViewBag.d2</h3>
                    <p>Count Of All Countries</p>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="orders" style="background-color:#3498db">

                <div class="icon-text">
                    <h3>@ViewBag.d3</h3>
                    <p>Sum Of All Flights Price</p>
                </div>
                <div class="clearfix"></div>
            </div>      
        </div>
    </div>
    <div class="clearfix"></div>
</body>
